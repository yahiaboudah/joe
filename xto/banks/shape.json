
{
    "simple": {
        "params": {"x": 0, "y": "function(){return this.x*2}"},
        "vertices": ["x", "y"]
    },

    "circle": {

        "params": {"radius": 8, "stretch": "function(){return this.radius / 1.81066;}"},

        "vertices"   : [["-radius",0],[0,"radius"],["radius",0],[0,"-radius"]],
        "inTangents" : [[0,"-radius"],["-stretch",0],[0,"stretch"],["stretch",0]],
        "outTangents": [[0,"stretch"],["stretch",0],[0,"-stretch"],["-stretch",0]],
        "closed"     : false
    }
}